{
  "version": 2,
  "framework": "nextjs",
  "functions": {
    "src/app/api/yandex-wordstat/**/*.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ],
  "github": {
    "silent": true
  }
}